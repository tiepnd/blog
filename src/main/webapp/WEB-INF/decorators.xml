<?xml version="1.0" encoding="UTF-8" ?>
<decorators defaultdir="/view/common">

    <excludes>
        <pattern>/api*</pattern>
    </excludes>
    <excludes>
        <pattern>/login*</pattern>
    </excludes>
    <decorator name="admin-layout" page="/WEB-INF/view/common/admin/layout.jsp">
        <pattern>/admin*</pattern>
    </decorator>
    <decorator name="user-layout" page="/WEB-INF/view/common/user/layout.jsp">
        <pattern>/*</pattern>
    </decorator>

</decorators>